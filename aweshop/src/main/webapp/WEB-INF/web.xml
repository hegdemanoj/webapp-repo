<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ItemsController</servlet-name>
  	<display-name>ItemsController</display-name>
  	<description></description>
  	<servlet-class>dataController.ItemsController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TestController</servlet-name>
  	<display-name>TestController</display-name>
  	<description></description>
  	<servlet-class>dataController.TestController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ItemsController</servlet-name>
  	<url-pattern>/ItemsController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TestController</servlet-name>
  	<url-pattern>/TestController</url-pattern>
  </servlet-mapping>
</web-app>
